package test;

class MyProgram {
	public static void main(String []args) {
		System.out.println("Hello World");
	      String message;
	        Scanner scan = new Scanner(System.in);      // 문자 입력을 인자로 Scanner 생성
	        
	        System.out.println("메시지를 입력하세요:");
	        
	        message = scan.nextLine();            // 키보드 문자 입력
	}
}
